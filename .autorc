{
    "onlyPublishWithReleaseLabel": false,
    "noDefaultLabels": true,
    "baseBranch": "main",
    "author": "Neurobagel Bot <bot@neurobagel.org>",
    "noVersionPrefix": false,
    "plugins": [
        ["npm", { "setRcToken": false }],
        "git-tag",
        "released",
        "conventional-commits",
        "first-time-contributor",
        [
            "omit-commits",
            {
                "username": ["dependabot[bot]", "pre-commit-ci[bot]"],
                "subject": ["[pre-commit.ci]", "[MNT] post-release version bump"],
                "labels": ["_bot", "skip-release"]
            }
        ]
    ]
}
